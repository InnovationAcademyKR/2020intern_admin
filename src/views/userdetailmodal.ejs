<div class="modal fade" id="userdetail" tabindex="-1" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">회원 상세정보</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="container-fluid">
          <form>
            <ul class="list-group list-group-flush">
              <li class="list-group-item">
                <div class="row">
                  <!-- 회원 이미지-->
                  <div class="col-md-6">
                    <img class="img-thumbnail" src="img/undraw_posting_photo.svg">
                  </div>
          
                  <!-- 회원 권한, 이름, 아이디 -->
                  <div class="col-md-6">
                    <ul class="list-gorup list-group-flush">
                      <!-- 회원 권한 -->
                      <li class="list-group-item">
                        <div class="form-row">
                          <div class="form-inline col-md-2">
                            <label>권한</label>
                          </div>
                          <div class="col-md-8">
                            <select class="form-control" disabled>
                                <option>일반</option>
                                <option>관리자</option>
                                <option>슈퍼 관리자</option>
                            </select>
                          </div>
                        </div>
                      </li>

                      <!-- 회원 이름 -->
                      <li class="list-group-item">
                        <div class="form-row">
                          <div class="form-inline col-md-2">
                            <lable>이름</lable>
                          </div>
                          <div class="form-inline col-md-6">
                            <input class="form-control" type="text" disabled>
                          </div>
                        </div>
                      </li>

                      <!-- 회원 아이디 -->
                      <li class="list-group-item">
                        <div class="form-row">
                          <div class="form-inline col-md-2">
                            <label>ID</label>
                          </div>
                          <div class="form-inline col-md-6">
                            <input class="form-control" type="text" disabled>
                          </div>
                        </div>
                      </li>
                    </ul>
                  </div>
                </div>
              </li>
              
              <!-- 회사 -->
              <li class="list-group-item">
                <div class="form-row">
                  <div class="form-inline col-md-2">
                    <label>회사</label>
                  </div>
                  <div class="col-md-10">
                    <input class="form-control" type="text" disabled>
                  </div>
                </div>
              </li>
              
              <!-- 소개글 -->
              <li class="list-group-item">
                <div class="form-row">
                  <div class="form-inline col-md-2">
                    <label>소개글</label>
                  </div>
                  <div class="col-md-10">
                    <textarea class="form-control" rows="5"></textarea>
                  </div>
                </div>
              </li>

              <!-- 경력 -->
              <li class="list-group-item">
                <div class="form-row">
                  <div class="col-md-2">
                    <label>경력</label>
                  </div>
                  <div class="col-md-10">
                    <% for(let i = 0; i < 10; ++i) { %>
                    <div class="input-group my-sm-1">
                      <input class="form-control" type="text" aria-describedby="deleteCareerButton" disabled>
                      <div class="input-group-append">
                        <button class="btn btn-outline-secondary" type="button" id="deleteCareerButton" disabled>삭제</button>
                      </div>
                    </div>
                    <% } %>
                    <div class="input-group">
                      <input class="form-control" type="text" aria-describedby="buttonAddon">
                      <div class="input-group-append">
                        <button class="btn btn-outline-secondary" type="button" id="buttonAddon">추가</button>
                      </div>
                    </div>
                  </div>
                </div>
              </li>

              <!-- 키워드 -->
              <li class="list-group-item">
                <div class="form-row">
                  <div class="form-inline col-md-2">
                    <label>키워드</label>
                  </div>
                  <div class="form-inline col-md-10">
                    <% for(let i = 0; i < 10; ++i) { %>
                      <button class="form-control badge badge-info" type="button">
                      <h5>
                        keyword
                      </h5>
                      <span>
                    <% } %>
                  </div>
                </div>
              </li>

              <!-- 매칭정보 -->
              <li class="list-group-item">
                <div class="form-row">
                  <div class="form-inline col-md-2">
                    <label>매칭정보</label>
                  </div>
                  <div class="form-inline">
                    <a href="#">열람하기</a>
                  </div>
                </div>
              </li>
            </ul>
          </form>
        </div>
      </div>
      <div class="modal-footer">
        <button class="btn btn-primary" type="button">수정</button>
        <button class="btn btn-primary" type="button">삭제</button> 
      </div>
    </div>
  </div>
</div>